{
    "builds": [{
        "src": "savemycode/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.3" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "savemycode/wsgi.py"
        }
    ],
      "functions": {
        "app_directory/wsgi.py": {
          "maxDuration": 60
    }
  }
}
